{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": {
        "jwt-auth": {
            "type": "package",
            "package": {
                "name": "hcktplanet/jwt-auth",
                "version": "1.0",
                "source": {
                    "url": "https://github.com/hcktplanet/jwt-auth.git",
                    "type": "git",
                    "reference": "origin/develop"
                }
            }
        },
        "laravel-jwt-impersonate": {
            "type": "package",
            "package": {
                "name": "hcktplanet/laravel-jwt-impersonate",
                "version": "1.0",
                "source": {
                    "url": "https://github.com/hcktplanet/laravel-jwt-impersonate.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    },
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-cors": "0.11.*",
        "cknow/laravel-money": "^3.7",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "flynsarmy/csv-seeder": "1.*",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.8.*",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "laravellegends/pt-br-validator": "5.1.*",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "moontoast/math": "1.1.*",
        "mpociot/laravel-apidoc-generator": "^3.7",
        "mtownsend/xml-to-array": "^1.0",
        "pragmarx/countries": "^0.6.0",
        "spatie/laravel-image-optimizer": "^1.4",
        "spatie/laravel-json-api-paginate": "^1.5",
        "hcktplanet/jwt-auth": "*",
        "hcktplanet/laravel-jwt-impersonate": "*"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "brianium/paratest": "^3.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "kkomelin/laravel-translatable-string-exporter": "^1.3",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "8"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
